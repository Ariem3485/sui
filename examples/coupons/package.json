{
    "name": "coupons",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"next dev\" \"npm:prettier:fix:watch\"",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --dir src && npm run prettier:check",
        "lint:fix": "next lint --dir src --fix && npm run prettier:fix",
        "prettier:check": "prettier -c --ignore-unknown .",
        "prettier:fix": "prettier -w --ignore-unknown .",
        "prettier:fix:watch": "npm run prettier:fix && onchange '**' --exclude-path .gitignore -- prettier -w --ignore-unknown {{file}}"
    },
    "dependencies": {
        "@faker-js/faker": "^6.1.2",
        "@reduxjs/toolkit": "^1.8.1",
        "animate.css": "^4.1.1",
        "bootstrap": "^5.1.3",
        "classnames": "^2.3.1",
        "formik": "^2.2.9",
        "next": "^12.1.2",
        "node-fetch": "^3.2.3",
        "rc-input-number": "^7.3.4",
        "react": "18.0.0",
        "react-apexcharts": "^1.4.0",
        "react-data-table-component": "^7.4.7",
        "react-dom": "18.0.0",
        "react-feather": "^2.0.9",
        "react-flatpickr": "^3.10.9",
        "react-hot-toast": "^2.2.0",
        "react-paginate": "^8.1.2",
        "react-perfect-scrollbar": "^1.5.8",
        "react-redux": "^7.2.8",
        "reactstrap": "^9.0.1",
        "sweetalert2": "^11.4.8",
        "sweetalert2-react-content": "^4.2.0",
        "swr": "^1.2.2",
        "uuid": "^8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/node": "17.0.23",
        "@types/react": "17.0.43",
        "@types/react-dom": "17.0.14",
        "@types/react-flatpickr": "^3.8.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "concurrently": "^7.1.0",
        "eslint": "^8.12.0",
        "eslint-config-next": "^12.1.2",
        "eslint-config-prettier": "^8.5.0",
        "onchange": "^7.1.0",
        "prettier": "^2.6.2",
        "react-select": "^5.2.2",
        "sass": "^1.49.9",
        "typescript": "^4.6.3"
    }
}
